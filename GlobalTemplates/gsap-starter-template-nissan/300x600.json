      "t": "s",
      "s": ".dynamicAdvertContainer{cursor:pointer;line-height:1;width:300px;height:600px}.template-set-frame{position:absolute;top:0;left:0;width:100%;height:100%}.devGuide{width:100%;height:100%;position:absolute;top:0;left:0;z-index:999;display:none}.devGuide.dev{opacity:$devoveropacity !important;display:block !important}@font-face{font-family:Nissan-Brand-Bold;src:url(https://s0.2mdn.net/creatives/assets/4152089/NissanBrand-Bold.woff) format('woff');font-weight:bold}@font-face{font-family:Nissan-Brand-Light;src:url(https://s0.2mdn.net/creatives/assets/4152089/NissanBrand-Light.woff) format('woff')}img{width:100%}.backGround{width:300px;height:600px;background-image:linear-gradient($gradientColor,transparent)}.absolutePos{position:absolute}.logo{top:18px;left:150px;width:88px;height:75px;transform:translateX(-50%)}.heading,.headingSub{width:260px;text-align:center}.heading{left:$headlineXPos1px;top:$headlineYPos1px;height:34px;font-family:$headlineWeight1;font-size:$headlineSize1px;letter-spacing:1.145px;color:$headlineColor1}.headingSub{left:$headlineXPos2px;top:$headlineYPos2px;height:44px;font-family:$headlineWeight2;font-size:$headlineSize2px;color:$headlineColor2}.cta{top:$ctaYPospx;left:$ctaXPospx;height:42px;padding:4px 60px 10px;background-color:#c40030;font-size:$ctaSizepx;display:flex;justify-content:center;align-items:center;gap:10px}.terms{top:$termsYPospx;left:$termsXPospx;width:252px;font-size:$termsSizepx;text-align:justify;color:#fff;display:$termsDisplay}.cta,.terms{font-family:Nissan-Brand-Light}"
